# Environment Variables Example
# Copy this to .env for local development

# Application
NODE_ENV=development
APP_PORT=3000

# Next.js Public Variables (accessible in browser)
NEXT_PUBLIC_STORYBLOK_TOKEN=your-storyblok-token-here
NEXT_PUBLIC_STORYBOOK_DISABLECACHING=false
NEXT_PUBLIC_MATOMO_TRACKER=false

# Server-side only variables (not accessible in browser)
NEXT_PUBLIC_STORYBLOK_EDITOR_SECRET=your-editor-secret-here
REVALIDATE_SECRET=your-revalidate-secret-here

# Development specific
DEBUG=true
LOG_LEVEL=debug

